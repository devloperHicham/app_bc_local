{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend-admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend-admin",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/datatables.net-dt/css/dataTables.dataTables.min.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "src/assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css",
              "src/assets/plugins/simplebar/css/simplebar.css",
              "src/assets/plugins/metismenu/css/metisMenu.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap-extended.css",
              "src/assets/css/style.css",
              "src/assets/css/icons.css",
              "src/assets/css/dark-theme.css",
              "src/assets/css/header-colors.css",
              "src/assets/plugins/select2/css/select2.min.css",
              "src/assets/plugins/select2/css/select2-bootstrap4.css",
              "src/assets/plugins/datetimepicker/css/classic.css",
              "src/assets/plugins/datetimepicker/css/classic.time.css",
              "src/assets/plugins/datetimepicker/css/classic.date.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/jquery.min.js",
              "node_modules/datatables.net/js/dataTables.min.js",
              "src/assets/plugins/simplebar/js/simplebar.min.js",
              "src/assets/plugins/metismenu/js/metisMenu.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js",
              "src/assets/plugins/apexcharts-bundle/js/apexcharts.min.js",
              "src/assets/plugins/chartjs/chart.min.js",
              "src/assets/js/index.js",
              "src/assets/plugins/select2/js/select2.min.js",
              "src/assets/js/form-select2.js",
              "src/assets/plugins/datetimepicker/js/legacy.js",
              "src/assets/plugins/datetimepicker/js/picker.js",
              "src/assets/plugins/datetimepicker/js/picker.time.js",
              "src/assets/plugins/datetimepicker/js/picker.date.js",
              "src/assets/plugins/bootstrap-material-datetimepicker/js/moment.min.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
            "options": {
            "host": "10.191.171.122",
            "ssl": true,
            "sslKey": "ssl/10.191.171.122-key.pem",
            "sslCert": "ssl/10.191.171.122.pem",
            "proxyConfig": "proxy.conf.json",
            "allowedHosts": ["10.191.171.122"]
          },
          "configurations": {
            "production": {
              "buildTarget": "frontend-admin:build:production"
            },
            "development": {
              "buildTarget": "frontend-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "cd62bf0a-3338-495a-ba88-b7099b6b9405"
  }
}